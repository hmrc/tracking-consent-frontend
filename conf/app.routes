# microservice specific routes

->         /govuk-frontend           govuk.Routes
->         /hmrc-frontend            hmrcfrontend.Routes
GET        /tracking.js              controllers.Assets.at(path="/public", file="tracking.js")
GET        /tracking-transitional.js controllers.Assets.at(path="/public", file="tracking-transitional.js")
GET        /cookie-settings          uk.gov.hmrc.trackingconsentfrontend.controllers.CookieSettingsController.cookieSettings(enableTrackingConsent: Option[String])
GET        /language/:lang           uk.gov.hmrc.trackingconsentfrontend.controllers.LanguageSwitchController.switchToLanguage(lang: String)
GET        /assets/*file             controllers.Assets.versioned(path = "/public", file: Asset)
